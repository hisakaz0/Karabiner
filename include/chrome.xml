<?xml version="1.0"?>
<root>
  <item>
    <name>Chrome</name>

    <item>
      <name>Command + K to Comand + Option + F</name>
      <name>Web site searching shortcut with windows os keybind(in link bar with ?)</name>
      <identifier>private.chrome.web.search</identifier>
      <only>Chrome</only>
      <autogen>
        __KeyToKey__
        KeyCode::K, ModifierFlag::COMMAND_L,
        KeyCode::F, ModifierFlag::COMMAND_L | ModifierFlag::OPTION_R
      </autogen>
    </item>

    <item>
      <name>History</name>
      <name>Opening history window with windows os keybind</name>
      <identifier>private.chrome.history</identifier>
      <only>Chrome</only>
      <autogen>
        __KeyToKey__
        KeyCode::H, ModifierFlag::COMMAND_L,
        KeyCode::Y, ModifierFlag::COMMAND_L
      </autogen>
    </item>

    <item>
      <name>Downloads</name>
      <name>Opening donwload page with windows os keybind</name>
      <identifier>private.chrome.downloads</identifier>
      <only>Chrome</only>
      <autogen>
        __KeyToKey__
        KeyCode::J, ModifierFlag::COMMAND_L,
        KeyCode::J, ModifierFlag::COMMAND_L | ModifierFlag::SHIFT_L
      </autogen>
    </item>

    <item>
      <name>Open Link</name>
      <name>Omni page searching shortcut with windows os keybind(in link bar)</name>
      <identifier>private.chrome.open.link</identifier>
      <only>Chrome</only>
      <autogen>
        __KeyToKey__
        KeyCode::L, ModifierFlag::COMMAND_L,
        KeyCode::L, ModifierFlag::COMMAND_L | ModifierFlag::SHIFT_L
      </autogen>
    </item>

  </item>
</root>

