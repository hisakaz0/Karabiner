<?xml version="1.0"?>
<root>
  <item>
    <name>window.session</name>

    <item>
      <name>Back</name>
      <identifier>private.window.session.back</identifier>
      <uielementrole_only>AXOutline</uielementrole_only>
      <autogen>
        __KeyToKey__
        KeyCode::B, KeyCode::BRACKET_LEFT | ModifierFlag::COMMAND_L
      </autogen>
    </item>

    <item>
      <name>Forward</name>
      <identifier>private.window.session.forward</identifier>
      <uielementrole_only>AXOutline,AXList</uielementrole_only>
      <autogen>
        __KeyToKey__
        KeyCode::P, KeyCode::BRACKET_RIGHT | ModifierFlag::COMMAND_L
      </autogen>
    </item>

    <item>
      <name>Open</name>
      <identifier>private.window.session.open</identifier>
      <block>
        <uielementrole_only>AXOutline</uielementrole_only>
        <autogen>
          __KeyToKey__
          KeyCode::L, KeyCode::CURSOR_DOWN | ModifierFlag::COMMAND_L
        </autogen>
      </block>
      <block>
        <uielementrole_only>AXList</uielementrole_only>
        <autogen>
          __KeyToKey__
          KeyCode::L, KeyCode::CURSOR_RIGHT
        </autogen>
        <autogen>
          __KeyToKey__
          KeyCode::SEMICOLON, KeyCode::CURSOR_DOWN | ModifierFlag::COMMAND_L
        </autogen>
      </block>
    </item>

    <item>
      <name>Enclosing Folder</name>
      <identifier>private.window.session.enclosing.folder</identifier>
      <block>
        <uielementrole_only>AXOutline</uielementrole_only>
        <autogen>
          __KeyToKey__
          KeyCode::H, KeyCode::CURSOR_UP | ModifierFlag::COMMAND_L
        </autogen>
      </block>
      <block>
        <uielementrole_only>AXList</uielementrole_only>
        <autogen>
          __KeyToKey__
          KeyCode::H, KeyCode::CURSOR_LEFT
        </autogen>
      </block>
    </item>

    <item>
      <name>Up Arrow</name>
      <identifier>private.window.session.up.arrow</identifier>
      <uielementrole_only>AXOutline,AXList</uielementrole_only>
      <autogen>
        __KeyToKey__
        KeyCode::K, KeyCode::CURSOR_UP
      </autogen>
    </item>

    <item>
      <name>Down Arrow</name>
      <identifier>private.window.session.down.arrow</identifier>
      <uielementrole_only>AXOutline,AXList</uielementrole_only>
      <autogen>
        __KeyToKey__
        KeyCode::J, KeyCode::CURSOR_DOWN
      </autogen>
    </item>

  </item>
</root>
